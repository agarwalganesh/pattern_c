#include <stdio.h>

int main() {
    int n;
    // printf("Enter the number of rows: ");
    scanf("%d", &n);

    for (int i = 1; i <= 5; i++) {
        for (int j = 1; j <= 2*(5-i+1)-1; j++) {
            if (j == 1 || j == 2*(5-i+1)-1 || i == 1)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}
